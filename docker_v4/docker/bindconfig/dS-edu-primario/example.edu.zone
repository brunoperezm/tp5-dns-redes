$TTL 1h
; -----------------------------------------------------------------------------
; SOA for example.edu      si no anda cambia contact por admin
; -----------------------------------------------------------------------------

@ IN SOA dS-edu-primario.example.edu admin.example.edu. (
            2016021501      ; Serial
            1h              ; Refresh
            15m             ; Retry
            1d              ; Expire
            1h );           ; Negative Cache TTL

