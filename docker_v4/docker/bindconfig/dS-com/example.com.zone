$TTL 1h
; -----------------------------------------------------------------------------
; SOA for example.com
; -----------------------------------------------------------------------------

@ IN SOA dS-com.example.com contact.example.com. (
            2016021501      ; Serial
            1h              ; Refresh
            15m             ; Retry
            1d              ; Expire
            1h );           ; Negative Cache TTL

; -----------------------------------------------------------------------------
; zone records
; -----------------------------------------------------------------------------
example.com.        1800    IN    NS        dS-com.example.com.

; -----------------------------------------------------------------------------
; pointer records
; -----------------------------------------------------------------------------
www                 1800    IN    A         192.168.1.50
dS-com.example.com.                 1800    IN    A         19.19.19.9
host3.example.com.                 1800    IN    A         19.19.19.8

; -----------------------------------------------------------------------------
; cname records
; -----------------------------------------------------------------------------
webmail             3600    IN    CNAME     www.example.com.