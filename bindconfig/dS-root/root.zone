$TTL 1h
; -----------------------------------------------------------------------------
; SOA for example.com
; -----------------------------------------------------------------------------

@ IN SOA dns.root root. (
            2016021503      ; Serial
            1h              ; Refresh
            15m             ; Retry
            1d              ; Expire
            1h );           ; Negative Cache TTL

; -----------------------------------------------------------------------------
; zone records
; -----------------------------------------------------------------------------
@                   IN  NS  dns.root.
example.com.        IN  NS  ns.example.com.
example.edu.        IN  NS  ns1.example.edu.
example.edu.        IN  NS  ns2.example.edu.

; -----------------------------------------------------------------------------
; pointer records
; -----------------------------------------------------------------------------
dns.root.           IN  A   19.19.17.9
ns.example.com.     IN  A   19.19.19.9
ns1.example.edu.     IN  A   19.19.18.9
ns2.example.edu.     IN  A   19.19.18.10

; -----------------------------------------------------------------------------
; cname records
; -----------------------------------------------------------------------------
